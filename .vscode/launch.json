{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch KelasGo API",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/kelasgo-api",
      "cwd": "${workspaceFolder}",
      "env": {
        "GIN_MODE": "debug"
      },
      "args": [],
      "showLog": true,
      "logOutput": "rpc",
      "trace": "verbose"
    },
    {
      "name": "Launch KelasGo API (Production Mode)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/kelasgo-api",
      "cwd": "${workspaceFolder}",
      "env": {
        "GIN_MODE": "release"
      },
      "args": [],
      "showLog": false
    },
    {
      "name": "Test Current Package",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${fileDirname}",
      "showLog": true
    },
    {
      "name": "Test All Packages",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceFolder}",
      "args": ["-v"],
      "showLog": true
    },
    {
      "name": "Attach to Process",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": 0
    }
  ]
}
